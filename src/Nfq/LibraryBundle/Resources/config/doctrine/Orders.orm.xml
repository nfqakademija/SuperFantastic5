<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Nfq\LibraryBundle\Entity\Orders" table="orders">
    <indexes>
      <index name="fk_Orders_Users1_idx" columns="reader_id"/>
      <index name="fk_Orders_Books1_idx" columns="book_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="id_UNIQUE" columns="id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="reservedAt" type="date" column="reserved_at" nullable="true"/>
    <field name="takenAt" type="date" column="taken_at" nullable="true"/>
    <field name="toReturnAt" type="date" column="to_return_at" nullable="true"/>
    <field name="returnedAt" type="date" column="returned_at" nullable="true"/>
    <many-to-one field="book" target-entity="Books">
      <join-columns>
        <join-column name="book_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="reader" target-entity="Users">
      <join-columns>
        <join-column name="reader_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
